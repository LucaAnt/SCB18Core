<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=========================== Food crops ==============================-->

	<ThingDef ParentName="PlantBase">
		<defName>PlantRice</defName>
		<label>rice plant</label>
		<description>A low grain plant that thrives in rich soil and yields food fast, but is not nice to eat uncooked.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/RicePlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<nutrition>0.18</nutrition>
		</ingestible>
		<plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<maxMeshCount>4</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawRice</harvestedThingDef>
			<harvestYield>7</harvestYield>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
		  <sowResearchPrerequisites>
		    <li>AgricultureI</li>
		  </sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<growDays>3</growDays>
			<visualSizeRange>
				<min>0.3</min>
				<max>0.8</max>
			</visualSizeRange>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
    <defName>PlantPotato</defName>
    <label>potato plant</label>
    <description>A simple, highly nutritious tuber. Grows well even in somewhat poor soil. Harvests quickly. A strong, versatile crop.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/PotatoPlant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <nutrition>0.20</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>0.4</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawPotatoes</harvestedThingDef>
			<harvestYield>14</harvestYield>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
      <sowResearchPrerequisites>
        <li>AgricultureI</li>
      </sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.8</growDays>
			<visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

  <ThingDef ParentName="PlantBase">
    <defName>PlantCorn</defName>
    <label>corn plant</label>
    <description>A large grain plant which produces ears of delicious yellow seeds. Takes a relatively long time to grow, but yields lots of food, and takes a long time to spoil. Needs fertile soil to grow well.</description>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/CornPlant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>12</pathCost>
    <ingestible>
      <nutrition>0.30</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawCorn</harvestedThingDef>
			<harvestYield>27</harvestYield>
			<sowTags>
				<li>Ground</li>
			</sowTags>
      <sowResearchPrerequisites>
        <li>AgricultureI</li>
      </sowResearchPrerequisites>
			<topWindExposure>0.35</topWindExposure>
			<growDays>11.3</growDays>
      <immatureGraphicPath>Things/Plant/CornImmature</immatureGraphicPath>
			<visualSizeRange>
        <min>0.3</min>
        <max>1.4</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

  <ThingDef ParentName="PlantBase">
    <defName>PlantStrawberry</defName>
    <label>strawberry plant</label>
    <description>A fragile but tasty fruit. Delicious even when raw.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/StrawberryPlant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <nutrition>0.20</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>0.6</fertilitySensitivity>
			<growDays>4.62</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawBerries</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<sowMinSkill>5</sowMinSkill>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
      <sowResearchPrerequisites>
        <li>AgricultureI</li>
      </sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
      <immatureGraphicPath>Things/Plant/StrawberryImmature</immatureGraphicPath>
			<visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

  <ThingDef ParentName="PlantBase">
    <defName>PlantHaygrass</defName>
    <label>haygrass</label>
    <description>A mixture of nutrient-rich grasses which yield large amounts of hay. Hay is edible for animals, but not humans.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Haygrass</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>5</pathCost>
    <ingestible>
      <nutrition>0.20</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>0.6</fertilitySensitivity>
			<growDays>7</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<maxMeshCount>4</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>Hay</harvestedThingDef>
			<harvestYield>23</harvestYield>
			<sowTags>
				<li>Ground</li>
			</sowTags>
      <sowResearchPrerequisites>
        <li>AgricultureI</li>
      </sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<visualSizeRange>
        <min>0.3</min>
        <max>0.8</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

  <!--=========================== Production crops ==============================-->

  <ThingDef ParentName="PlantBase">
    <defName>PlantCotton</defName>
    <label>cotton plant</label>
    <description>Yields cotton, a fine plant fiber used to make cloth.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/CottonPlant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <nutrition>0.20</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<growDays>8</growDays>
			<dieIfLeafless>true</dieIfLeafless>
      <reproduces>false</reproduces>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawCotton</harvestedThingDef>
      <harvestYield>13</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <sowResearchPrerequisites>
        <li>Devilstrand</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.1</topWindExposure>
      <immatureGraphicPath>Things/Plant/CottonImmature</immatureGraphicPath>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

  <ThingDef ParentName="PlantBase">
    <defName>PlantDevilstrand</defName>
    <label>devilstrand</label>
    <description>This fragile mushroom was genetically engineered to produce a silk net of the highest grade. Long rumored to have mystical properties, it owes its name to the greed it inspires in people.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Devilstrand</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <nutrition>0.15</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<growDays>22.5</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawDevilstrand</harvestedThingDef>
			<harvestYield>8</harvestYield>
      <neverBlightable>true</neverBlightable>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<sowMinSkill>10</sowMinSkill>
			<sowResearchPrerequisites>
				<li>Devilstrand</li>
			</sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<visualSizeRange>
        <min>0.3</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

  <ThingDef ParentName="PlantBase">
    <defName>PlantHealroot</defName>
    <label>healroot</label>
    <description>A slow-growing plant which yields herbal medicine when harvested.\n\nHealroot was selectively bred for centuries by settlers. It is sometimes also called &quot;healer's hand&quot; by tribespeople - a nickname it owes to its five major medicinal virtues.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Healroot</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <nutrition>0.15</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<growDays>6.5</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>HerbalMedicine</harvestedThingDef>
			<harvestYield>1</harvestYield>
			<sowMinSkill>8</sowMinSkill>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<sowResearchPrerequisites>
			  <li>HospitalBed</li>
      </sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<visualSizeRange>
        <min>0.3</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  
  
  <ThingDef ParentName="PlantBase">
    <defName>PlantGEF</defName>
    <label>G.E.F. 728-394 plant</label>
    <description>A genetically engineered plant designed to provide the whole nutritional value spectrum while maximizing resistence, growth rate and taste. However contain some extra substances that may cause addiction.</description>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plants/DragonFruitPlant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.8</hideAtSnowDepth>
    <selectable>true</selectable>
    <pathCost>50</pathCost>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
    <plant>
      <fertilityMin>0.05</fertilityMin>
      <fertilitySensitivity>0.3</fertilitySensitivity>
      <reproduceRadius>10</reproduceRadius>
      <dieIfLeafless>false</dieIfLeafless>
      <reproduces>true</reproduces>
      <harvestTag>Standard</harvestTag>
      <maxMeshCount>4</maxMeshCount>
      <harvestedThingDef>RawGEF</harvestedThingDef>
      <harvestYield>4</harvestYield>
     <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <sowResearchPrerequisites>
        <li>AgricultureV</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.1</topWindExposure>
      <growDays>2</growDays>
      <reproduceMtbDays>0.1</reproduceMtbDays>
      <harvestWork>300</harvestWork>
      <visualSizeRange>
        <min>0.5</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  
</Defs>
